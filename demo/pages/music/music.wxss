.search {
  background-color: #fff;
  border-radius: 40rpx;
  margin:0 20rpx;
  padding:8rpx 30rpx;
  border:2rpx #BBBBBB solid;
  font-size:25rpx;
  position: fixed;
  top: 0;
  left:0;
  right:0;
}


.container {
  display: flex;
  flex-direction: column;
  padding:5rpx 20rpx;
  margin-top: 60rpx;
  margin-bottom: 100rpx;
}

.item {
  border-bottom: 1rpx #BBBBBB solid;
  padding:20rpx;
}

.audio audio {
  position: fixed;
  bottom: 0;
  left:0;
  right:0;
  text-align:center;
  background: #fff;
}
